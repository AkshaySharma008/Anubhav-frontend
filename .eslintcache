LTags = (str) => {\r\n        str = str.replace(/&lt;/g, '<').replace(/&gt;/g, '>').replace(/&amp;/g, '&');\r\n        return str\r\n    }\r\n\r\n    const parseDate = (str) => {\r\n        let dt = new Date(str);\r\n        return dt.getDate() + \"/\" + (dt.getMonth() + 1) + \"/\" + dt.getFullYear();\r\n    }\r\n    return (\r\n        <div className=\"col-12 d-flex justify-content-around article-content mt-4\">\r\n            { (loading) ? (<Loading />\r\n            ) : (\r\n\r\n\r\n                    <div className=\"flex-self-center article-body\">\r\n                        <h1 className=\"content-heading mt-4 mb-1 col-12 text-left\"> {articleData.title}</h1>\r\n                        <div className=\"content-author-detail col-12 mb-1 text-left\">by {articleData.author.name} &nbsp; |&nbsp; {parseDate(articleData.createdAt)} &nbsp; |&nbsp;\r\n                        <Link to={`/interview/${articleData.companyName}`}>{articleData.companyName}</Link>\r\n                        </div>\r\n                        <div className=\"tags mb-4 \">\r\n                            {\r\n                                articleData.articleTags.map((entity, index_e) =>\r\n                                    <Badge variant='secondary' key={index_e} >{entity}</Badge>\r\n\r\n                                )\r\n                            }\r\n                        </div>\r\n                        <div className=\"content-paragraph col-12 text-left article-post\">\r\n                            {parse(parseHTMLTags(articleData.description))}\r\n                        </div>\r\n                    </div>\r\n                )}\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Article\r\n","C:\\Users\\dell\\Desktop\\ANUBHAV\\Anubhav-frontend\\src\\pages\\AboutCreators\\index.jsx",[],"C:\\Users\\dell\\Desktop\\ANUBHAV\\Anubhav-frontend\\src\\pages\\HomePage\\index.jsx",[],"C:\\Users\\dell\\Desktop\\ANUBHAV\\Anubhav-frontend\\src\\pages\\CompanyWiseArticleList\\index.jsx",[]